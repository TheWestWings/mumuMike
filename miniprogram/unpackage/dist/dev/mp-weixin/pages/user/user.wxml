<view class="container"><view class="header"><block wx:if="{{userInfo.username}}"><view class="user-info"><image class="avatar" src="/static/logo.png"></image><text class="username">{{userInfo.username}}</text></view></block><block wx:else><view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="user-info" bindtap="__e"><image class="avatar" src="/static/logo.png"></image><text class="username">点击登录</text></view></block></view><view class="section"><view class="section-header"><text class="title">我的订单</text></view><view class="order-list"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['orderList','id',item.$orig.id,'id']]]]]]]}}" class="order-item" bindtap="__e"><view class="order-top"><text class="time">{{item.m0}}</text><text class="status">{{item.m1}}</text></view><view class="order-content"><text>{{"共 "+(item.$orig.product?item.g0:0)+" 件商品"}}</text></view></view></block><block wx:if="{{$root.g1===0}}"><view class="empty"><text>暂无订单</text></view></block></view></view><view class="section"><button data-event-opts="{{[['tap',[['goFeedback',['$event']]]]]}}" class="feedback-btn" bindtap="__e">意见反馈</button><block wx:if="{{userInfo.username}}"><button data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" class="logout-btn" bindtap="__e">退出登录</button></block></view></view>